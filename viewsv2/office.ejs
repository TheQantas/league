              <div class="chatHomeMess" <% if (r.none) { %>style="font-style:italic;"<% } %>><%= r.mess %></div>
              <div class="chatHomeDate"><%= r.since %></div>
            </div>
          <% } %>
        </section>
      <% } %>
      <section class="page" id="settCont" <% if (mobile) {%>style="display:block;height:calc(100vh - 60px);"<%}%>> <!-- settings -->
        <% if (mobile) { %>
          <div id="limited">You are viewing a limited mobile version</div>
        <% } %>
        <div id="settTop">
          <div class="<%= clean %>"><img src="logos/<%= clean %>.png"></div>
        </div>
        <div id="settWelcome">Welcome, <%= mascot %>!</div>
        <div id="settSignCont">
          <button onclick="signOut()">Sign Out</button>
          <button onclick="univSignOut()">Sign Out of All Devices</button>
        </div>
        <div class="settDivider">Change Password</div>
        <div class="settGrid">
          <input type="password" oninput="newPass()" id="newPass" placeholder="New Password">
          <input type="password" oninput="newPass()" id="repeatPass" placeholder="Repeat New Password">
          <input type="password" id="currentPass" placeholder="Current Password">
          <button onclick="newPass(true)">Change Password</button>
        </div>
        <div class="settError" id="changePassError"></div>
        <div class="settDivider">Verify Email</div>
        <% if (emailActivated) { %>
          <div class="settNote">You have verified your email. <a onclick="ws.mail({method:'removeEmail'})">Remove Email</a></div>
          <div class="settError" id="removeEmailError"></div>
        <% } else { %>
          <div class="settNote">Your email will only be used for account recovery and notification 
            of upcoming The League seasons.</div>
          <div class="settGrid">
            <input type="email" id="email" placeholder="Enter Email">
            <input type="password" id="emailPassword" placeholder="Enter Password">
            <button onclick="verifyEmail()" style="grid-column:1/span 2;">Verify Email</button>
            <input type="number" oninput="verifyCode(this.value)" id="emailCode" placeholder="######">
          </div>
          <div class="settError" id="verifyEmailError"></div>
          <div class="settError" id="verifyCodeError"></div>
        <% } %>
        <div class="settDivider">Two-Factor Authentication</div>
        <% if (factorActivated) { %>
          <div class="settNote">You have activated two-factor authentication. <a onclick="ws.mail({method:'removeFactor'})">Remove Two-Factor Authentication</a></div>
          <div class="settError" id="removeFactorError"></div>
        <% } else { %>
          <div class="settGrid">
            <input type="password" id="factorPassword" placeholder="Enter Password">
            <button onclick="getQRCode()">Get QR Code</button>
          </div>
          <div id="qrCont"><div><img id="qrCode"></div></div>
          <div class="settGrid" id="factorCodeCont" style="display:none;">
            <input type="number" oninput="factorCode(this.value)" id="factorCode" placeholder="######">
          </div>
          <div class="settError" id="enable2faError"></div>
          <div class="settError" id="verify2faError"></div>
        <% } %>
        <div class="settDivider">Recovery Codes</div>
        <div id="codesExp">These are single-use codes that can be used to recover your account. Make sure you save these codes in a safe place.</div>
        <div class="settGrid" id="codes">
          <div class="codeCell" style="display:none;"></div>
          <div class="codeCell" style="display:none;"></div>
          <div class="codeCell" style="display:none;"></div>
          <div class="codeCell" style="display:none;"></div>
          <div class="codeCell" style="display:none;"></div>
          <div class="codeCell" style="display:none;"></div>
          <div class="codeCell" style="display:none;"></div>
          <div class="codeCell" style="display:none;"></div>
          <button style="grid-column: 1/span 4;" onclick="ws.mail({method:'getCodes'})">Request Codes</button>
        </div>
      </section>
    </main>
    <div id="errorCont">
      <span></span>
    </div>
    <% if (dark != 'true' && prompt != 'false') { %>
      <div id="promptCont">
        <div id="prompt">
          <div>Enable Dark Mode?</div>
          <div>
            <button onclick="enableDark()">Yes</button>
            <button onclick="declineDark()" style="margin-left:5px;">No</button>
          </div>
        </div>
      </div>
    <% } %>
  </body>
</html>